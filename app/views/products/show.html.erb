<% will_paginate @products %>
<h1>Poducts</h1>
<table>
<tr>
<th>Product Name</th>
<th>Category</th>
</tr>

<% @products.each do |product| %>
<tr>
<td><%= link_to product.productName, controller: "product_details", action: "show", id: product%></td>
<td><%= product.category_id %></td>
</tr>
<% end %>
</table>
<% will_paginate @products %>